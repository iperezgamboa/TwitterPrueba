<p id="notice"><%= notice %></p>

<h1>Hola</h1>

<%= link_to  "cerrar sesion", destroy_user_session_path, method: :delete %>
<%= link_to  "Iniciar sesion", new_user_session_path %>
<%= link_to  "Crear cuenta", new_user_registration_path %>

<h1>Tweets</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.content %></td>
        <td><%= tweet.user.email %></td>
        <td><%= link_to 'Show', tweet %></td>
        <td><%= link_to 'Edit', edit_tweet_path(tweet) %></td>
        <td><%= link_to 'Destroy', tweet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
   </tbody>
</table>

<br>

<%= link_to 'New Tweet', new_tweet_path %>
